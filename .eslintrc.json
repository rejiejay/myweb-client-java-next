{
    "extends": "react-app",
    "globals": { 
        "wx": true 
    },
    "rules": { 
        "no-undef": "off",
        "no-restricted-globals": "off",
        "no-unused-vars": "off",
        "no-useless-escape": "off",
        "no-sequences": "off",
        "new-parens": "off",
        "eqeqeq": "off", 
        "no-dupe-keys": "off",
        "no-mixed-operators": "off",
        "no-unused-expressions": "off"
    }
}
